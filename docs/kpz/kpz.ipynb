    "try:\n",
    "    import qsimcirq\n",
    "except ImportError:\n",
    "    !pip install qsimcirq --quiet\n",
    "    import qsimcirq\n",
    "\n",
    "import cirq\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib import ticker\n",
